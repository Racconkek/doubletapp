{
  "name": "doubletapp-students",
  "version": "1.0.0",
  "description": "doubletapp test - STUDENTS",
  "main": "src/server/index.js",
  "engines": {
    "node": "13"
  },
  "scripts": {
    "build": "npm-run-all build:server build:client",
    "build:client": "webpack --mode=production",
    "build:server": "copyfiles --up 1 \"./src/server/**/*\" \"./src/assets/**/*\" dist/",
    "dev:build:client": "webpack --mode=development",
    "dev:build:server": "copyfiles --up 1 \"./src/server/**/*\" \"./src/assets/**/*\" dist/",
    "dev:start": "cross-env NODE_ENV=development node dist/server/index.js",
    "dev": "npm-run-all dev:build:* dev:start",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint src/client/**/*.css",
    "lint:js": "eslint src/server/**/*.js",
    "lint:jsx": "eslint src/client/**/*.jsx",
    "deps:all": "npm ci",
    "deps:production": "npm ci --production",
    "docker:build": "docker build --tag %npm_package_name% .",
    "docker:run": "docker run -p 3000:80 %npm_package_name%",
    "heroku-init": "heroku login && heroku create %npm_package_name%",
    "heroku:login": "heroku container:login",
    "heroku:push": "heroku container:push web --app %npm_package_name%",
    "heroku:release": "heroku container:release web --app %npm_package_name%",
    "heroku:open": "heroku open --app %npm_package_name%",
    "heroku:logs": "heroku logs --tail --app %npm_package_name%",
    "heroku": "npm-run-all heroku:*",
    "start": "cross-env NODE_PATH=dist/server/ node dist/server/",
    "deploy": "npm-run-all build heroku"
  },
  "keywords": [],
  "author": "Miroshnichenko",
  "license": "ISC",
  "dependencies": {
    "config": "^3.3.1",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "mongodb": "^3.5.9",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "copyfiles": "^2.3.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.5.0",
    "eslint-config-xo": "^0.32.1",
    "eslint-plugin-react": "^7.20.5",
    "file-loader": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
